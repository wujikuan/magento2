<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Zou_BannerSlider::bannerslider" title="Zou Banner Slider" sortOrder="55">
                    <resource id="Zou_BannerSlider::banner" title="Manage Banner" sortOrder="10"/>
                    <resource id="Zou_BannerSlider::slider" title="Manage Slider" sortOrder="20"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Zou_BannerSlider::bannerslider_config" title="Zou Banner Slider"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
